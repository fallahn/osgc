audioscape
{
    emitter music_day
    {
        path = "assets/sound/music/day.ogg"
        streaming = true
        looped = true
        volume = 1.2
        mixer_channel = 1
    }

    emitter music_night
    {
        path = "assets/sound/music/night.ogg"
        streaming = true
        looped = true
        volume = 1.9
        mixer_channel = 1
    }    
}